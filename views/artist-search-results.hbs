<div class=" container">
    <h1 class="title">Resultado de la busqueda</h1>
    <hr>
    <div class="row">

        {{#each artist}}

        <div class="col-md-4 d-flex align-items-stretch">
            <div class="card mb-4">
                <img src="{{images.1.url}}" class="card-img-top" alt="{{name}}">
                <div class="card-body">
                    <h5 class="card-title">{{name}}</h5>
                </div>
            </div>
            <a href="/albums/{{id}}" class="btn btn-dark">View Albums</a>
        </div>

        {{/each}}

    </div>

</div>